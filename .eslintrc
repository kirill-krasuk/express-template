{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion":  2018, 
        "sourceType":  "module"
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "airbnb-base",
        "plugin:varspacing/recommended"
    ],
    "rules": {
        "@typescript-eslint/consistent-type-assertions": 0,
    },
    "plugins": [
        "arca",
        "varspacing"
    ]
}
